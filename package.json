{
  "name": "fast-case",
  "version": "1.2.1",
  "description": "A fast C++ library for camelcase and pascalcase",
  "author": "Jacob Gillespie <jacobwgillespie@gmail.com>",
  "homepage": "https://github.com/jacobwgillespie/fast-case",
  "repository": "git://github.com/jacobwgillespie/fast-case.git",
  "license": "MIT",
  "main": "dist/fastCase.js",
  "module": "dist/fastCase.mjs",
  "source": "src/index.mjs",
  "typings": "./fast-case.d.ts",
  "files": [
    "dist",
    "binding.gyp",
    "package.json",
    "fast-case.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "benchmark": "node support/benchmark.js",
    "build": "microbundle",
    "clean": "rm -rf dist",
    "dev": "microbundle watch",
    "format": "prettier --write './src/**/*.{js,mjs}' && clang-format -i src/fast-case.cc src/algorithms.hpp",
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild:build": "prebuild --all --strip --verbose",
    "prebuild:rebuild": "prebuild --compile",
    "prepare": "npm run clean && npm run build",
    "test": "ava"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "nan": "^2.11.0",
    "prebuild-install": "^5.1.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "benchmark": "^2.1.4",
    "clang-format": "^1.2.4",
    "humps": "^2.0.1",
    "microbundle": "^0.6.0",
    "prebuild": "^8.0.1",
    "prebuild-ci": "^2.2.3",
    "prettier": "^1.14.2",
    "xcase": "^2.0.1"
  }
}
